<odoo>
    <data noupdate="1">
        <!-- Sequence cho Số Serial Tài Sản -->
        <record id="seq_tai_san_serial" model="ir.sequence">
            <field name="name">Sequence Số Serial Tài Sản</field>
            <field name="code">tai.san.serial</field>
            <field name="prefix">TS-SERIAL-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho Lịch Sử Sử Dụng -->
        <record id="seq_lich_su_su_dung" model="ir.sequence">
            <field name="name">Sequence Lịch Sử Sử Dụng</field>
            <field name="code">lich_su_su_dung</field>
            <field name="prefix">LS-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho Lịch Sử Bảo Trì -->
        <record id="seq_lich_su_bao_tri" model="ir.sequence">
            <field name="name">Sequence Lịch Sử Bảo Trì</field>
            <field name="code">lich_su_bao_tri</field>
            <field name="prefix">LSBT-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho Vị trí -->
        <record id="seq_vi_tri" model="ir.sequence">
            <field name="name">Sequence Vị trí</field>
            <field name="code">vi_tri</field>
            <field name="prefix">VT-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
